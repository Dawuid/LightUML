<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.lightuml.test"
   class="org.lightuml.test.LightUMLTestPlugin"
   name="LightUML Unit Test Plug-in"
   version="1.0.0"
   provider-name="LightUML">

   <runtime>
      <library name="test.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.lightuml.core"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ant.core"/>
      <import plugin="org.eclipse.core.resources"/>
   </requires>

</plugin>
